{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "NITRO_PRESET=vercel nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "exit 0",
    "start": "node .output/server/index.mjs",
    "vercel-build": "npm install @rollup/wasm-node @esbuild/linux-x64 --no-save && nuxt prepare && npm run generate"
  },
  "dependencies": {
    "ace-builds": "^1.37.4",
    "nuxt": "^3.15.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/node": "^20.17.5",
    "typescript": "^5.6.3"
  },
  "imports": {
    "#internal/nuxt/paths": "./node_modules/nuxt/dist/core/runtime/nitro/paths"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    },
    "esbuild": "^0.19.12"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node"
  }
}