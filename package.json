{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NITRO_PRESET=vercel nuxt build",
    "dev": "NODE_OPTIONS=--no-warnings node ./node_modules/.bin/nuxt dev",
    "dev-local": "NITRO_PRESET=node NODE_OPTIONS=--no-warnings nuxt dev",
    "generate": "NITRO_PRESET=vercel nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "exit 0",
    "start": "node .output/server/index.mjs",
    "static": "cp -r static-site/* .output/public/",
    "clean": "rm -rf node_modules/.vite .nuxt .output",
    "clean-scripts": "mkdir -p backup-scripts && mv build-render*.sh deploy.sh static-only-build.sh simple-build.sh vercel-build-fixed.sh deploy-vercel.sh vercel-build.js render.yaml deploy-fix.sh backup-scripts/ 2>/dev/null || true"
  },
  "dependencies": {
    "@agentdeskai/browser-tools-server": "^1.1.0",
    "ace-builds": "^1.37.4",
    "esbuild": "0.25.0",
    "nuxt": "^3.15.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/node": "^20.17.5",
    "typescript": "^5.6.3"
  },
  "imports": {
    "#internal/nuxt/paths": "./node_modules/nuxt/dist/core/runtime/nitro/paths"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "trustedDependencies": [
    "esbuild",
    "rollup"
  ],
  "overrides": {
    "esbuild": "0.25.0"
  },
  "resolutions": {
    "esbuild": "0.25.0",
    "vite-node/esbuild": "0.25.0",
    "vite/esbuild": "0.25.0"
  }
}
