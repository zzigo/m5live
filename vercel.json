{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "headers": [
    {
      "source": "/_nuxt/([^/]+)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/_nuxt/([^/]+)\\.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
} 